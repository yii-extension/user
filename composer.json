{
    "name": "yii-extension/user",
    "type": "library",
    "description": "Flexible user registration and authentication module for Yii3",
    "keywords": [
        "yii3",
        "application",
        "active-record",
        "user-registration",
        "user-authentication"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "lasserafn/php-initial-avatar-generator": "^4.1",
        "yii-extension/url-service": "^1.0.0",
        "yii-extension/user-mailer-service": "^1.0.0",
        "yii-extension/user-settings": "^1.0.0",
        "yii-extension/user-view-implementation": "^1.0.0",
        "yiisoft/active-record": "^3.0@dev",
        "yiisoft/aliases": "^2.0",
        "yiisoft/auth": "^2.0|^3.0",
        "yiisoft/data-response": "^1.0.0",
        "yiisoft/files": "^1.0.0",
        "yiisoft/form": "^1.0@dev",
        "yiisoft/http": "^1.1",
        "yiisoft/log-target-file": "^1.0.0",
        "yiisoft/log": "^1.0.0",
        "yiisoft/middleware-dispatcher": "^1.0@dev",
        "yiisoft/router-fastroute": "^3.0@dev",
        "yiisoft/router": "^3.0@dev",
        "yiisoft/session": "^1.0.0",
        "yiisoft/translator-formatter-intl": "^1.0.0",
        "yiisoft/user": "^3.0@dev",
        "yiisoft/yii-db-migration": "^1.0@dev"
    },
    "require-dev": {
        "codeception/c3": "^2.6.0",
        "codeception/codeception": "^4.1.20",
        "codeception/module-asserts": "^1.3.1",
        "codeception/module-db": "^1.1",
        "codeception/module-phpbrowser": "^1.0.2",
        "nyholm/psr7": "^1.3",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.8",
        "yiisoft/config": "^1.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/db-sqlite": "^3.0@dev",
        "yiisoft/error-handler": "^1.0.0",
        "yiisoft/yii-web": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\User\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\User\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "common": [
                "config/common/user.php"
            ],
            "events": "config/events.php",
            "routes": "config/routes.php"
        }
    },
    "config": {
        "sort-packages": true
    }
}
